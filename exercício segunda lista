void  primo () { // alterar um número é primo ou não
	int x, resultado;

	printf ( " Digite um número para saber se ele é o primeiro: \ n " );
	scanf ( " % d " , & x);
	resultado = x% 1 || x% x;
	if (resultado < 1 && resultado <x! = 0 ) {
		printf ( " e um número primo! \ n " );
	}
	mais {
		printf ( " Nao e um numero primo! \ n " );
	}
}	
void  fatorial () { // imprimindo o numero fatorial
	int x, y;

	printf ( " Digite um número \ n " );
	scanf ( " % d " , & x);
	y = x;
	y = y - 1 ;
	para (y = y; y> 0 ; y -) {
		x = x * y;
	}
	printf ( " % f " , x);
}
void  fibonate () { // sequência de fibonate
	int n, n1 = 0 , n2 = 1 , x;

	printf ( " Digite um número: " , n);
	scanf ( " % d " , & n);

	while (n! = 0 ) {

 	printf ( " % d " , x);
		x = n1 + n2;
		n2 = n1;
		n1 = x;
		n--;
	}
}
